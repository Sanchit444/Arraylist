using System;
class Exp
{
    public static void Main(String[] args)
    {
       Console.WriteLine("Enter the lengths of the sides of the triangle:");
       Console.WriteLine("Side 1: ");
       double side1 = Convert.ToDouble(Console.ReadLine());
         Console.WriteLine("Side 2: ");
        double side2 = Convert.ToDouble(Console.ReadLine());
        Console.WriteLine("Side 3: ");
        double side3 = Convert.ToDouble(Console.ReadLine());
        if(side1+side2 > side3 &&  side1+side3 > side2 && side2+side3 > side1)
        {
if(side1==side2 && side2 == side3)
             Console.WriteLine("Triangle is equilateral");
             else if(side1 == side2 || side1 == side3 || side2 == side3)
             Console.WriteLine("Triangle is isosceles"); 
             else Console.WriteLine("Triangle is scalene");
        }
        else
        Console.WriteLine("Triangle is not constructible");
        }
    }



